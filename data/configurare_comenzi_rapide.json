[
   {
		"type": "TextBubble", 
		"delay": "0", 
		"text": "Sfatul zilei: poți personaliza comenzile rapide, încât să conțină doar opțiunile importante pentru tine.", 
		"sender": "chatbot"
	},
    {
		"type": "TextBubble", 
		"delay": "3",
		"text": "Vrei să personalizezi comenzile rapide?", 
		"sender": "chatbot"
	},
	{
		"type": "QuickReplies", 
		"delay": "1", 
		"replies":[
			{"reply": "⚙️ Da, personalizare"}, 
			{"reply": "⏩ Sari pasul acesta"}
		]
	},
	{
		"type": "MockEvent",
		"delay": "2",
		"event":
		{
			"type":"scroll-and-tap",
			"scrollindex": "0",
			"tapindex": "0",
			"tapDelay": "1"
		}
    },	
	{
		"type": "WebView", 
		"delay": "3",
		"title": "Comenzi Rapide",
		"content": "http://demo.imprezzioglobal.com/Vodafone/Chatbot_03/configurare-comenzi-rapide.html"
	},
	{
		"type": "MockEvent",
		"delay": "4",
		"event":
		{
			"type":"send-custom-js-to-webview",
			"customJs": "myApp.smartSelectOpen('.smart-select');"
		}
	},	
	{
		"type": "MockEvent",
		"delay": "2",
		"event":
		{
			"type":"send-custom-js-to-webview",
			"customJs": "$('input[type=\"checkbox\"]')[0].checked = true;"
		}
	},
	{
		"type": "MockEvent",
		"delay": "2",
		"event":
		{
			"type":"send-custom-js-to-webview",
			"customJs": "$('input[type=\"checkbox\"]')[5].checked = true;"
		}
	},	
	{
		"type": "MockEvent",
		"delay": "2",
		"event":
		{
			"type":"send-custom-js-to-webview",
			"customJs": "$('input[type=\"checkbox\"]')[8].checked = true;"
		}
	},
	{
		"type": "MockEvent",
		"delay": "3",
		"event":
		{
			"type":"close-webview"
		}
	},
	{
		"type": "TextBubble", 
		"delay": "1", 
		"typingDelay": "1",
		"text": "Alege:", 
		"sender": "chatbot"
	},
	{
		"type": "QuickReplies", 
		"delay": "2", 
		"replies":[
			
			{"reply": "Factura curenta"}, 
			{"reply": "Cost control"},
			{"reply": "Reincarcare"},  
			{"reply": "Setare Comenzi Rapide"},
            {"reply": "⬅️ Meniu principal"}
		]
	},
	{
		"type": "MockEvent",
		"delay": "3",
		"event":
		{
			"type":"scroll-and-tap",
			"scrollindex": "4",
			"tapindex": "4",
			"tapDelay": "3"
		}
	},
	{
		"type": "TextBubble", 
		"delay": "4",
		"typingDelay": "2", 
		"text": "Sfarsit simulare", 
		"sender": "chatbot"
	}

]